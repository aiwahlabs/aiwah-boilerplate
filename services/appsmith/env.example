# Appsmith Configuration
# Copy this file to .env and fill in your actual values

# Encryption settings (optional - Appsmith will generate if not provided)
# Generate random 32+ character strings for these values
APPSMITH_ENCRYPTION_PASSWORD=your-encryption-password-here
APPSMITH_ENCRYPTION_SALT=your-encryption-salt-here

# Optional: Custom domain configuration
# APPSMITH_CUSTOM_DOMAIN=your-domain.com

# Optional: Email configuration (for notifications and password reset)
# APPSMITH_MAIL_ENABLED=true
# APPSMITH_MAIL_FROM=noreply@your-domain.com
# APPSMITH_MAIL_HOST=smtp.gmail.com
# APPSMITH_MAIL_PORT=587
# APPSMITH_MAIL_USERNAME=your-email@gmail.com
# APPSMITH_MAIL_PASSWORD=your-app-password

# Optional: OAuth configuration
# APPSMITH_OAUTH2_GOOGLE_CLIENT_ID=your-google-client-id
# APPSMITH_OAUTH2_GOOGLE_CLIENT_SECRET=your-google-client-secret
# APPSMITH_OAUTH2_GITHUB_CLIENT_ID=your-github-client-id
# APPSMITH_OAUTH2_GITHUB_CLIENT_SECRET=your-github-client-secret 